<div class="ui dropdown item">
Account <i class="dropdown icon"></i>
<div class="menu">
  <% if logged_in? %>
    <%= link_to "Logout", appout_path, method: :delete, class: "item" %>
  <% else %>
    <%= link_to "Log in", app_path, class: "item" %>
  <% end %>
</div>
</div>

<% if !logged_in? %>
<div class="item">
  <%= link_to "Log in", app_path, class: "ui primary button" %>
</div>
<% end %>
</div>